<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>


<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="souchon.game.controller.HomeController" >

    <VBox xmlns="http://javafx.com/javafx/11.0.1"
          xmlns:fx="http://javafx.com/fxml/1">
        <AnchorPane VBox.vgrow="ALWAYS">
            <ImageView pickOnBounds="true" preserveRatio="true">
                <Image url="/images/HomePage.png"/>
            </ImageView>
            <Button layoutX="230.0" layoutY="176.0" mnemonicParsing="false" onAction="#clickPlay" fx:id="playBtn" text="JOUER">
                <font>
                    <Font name="Eras Bold ITC" size="21.0" />
                </font>
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
            </Button>
            <TextField layoutX="217.0" layoutY="259.0" promptText="Pseudo" fx:id="tempsTxt"/>
        </AnchorPane>
    </VBox>

</AnchorPane>
